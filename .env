# HTTP服务器地址# Temp Mail 配置文件

HTTP_ADDR=:8080

# HTTP 服务端口

# SMTP服务器地址HTTP_ADDR=:8080

SMTP_ADDR=:25

# SMTP 接收/发送服务端口（标准邮件端口）

# 域名（可以是IP地址或域名）SMTP_ADDR=:25

DOMAIN=127.0.0.1

# 邮件域名（部署时修改为实际域名或IP）

# 消息保留时间（秒）# 示例: mail.example.com 或 101.44.160.108

MESSAGE_TTL=3600DOMAIN=mail.example.com



# 时区# 邮件保留时间

TZ=Asia/ShanghaiMESSAGE_TTL=30m


# 时区
TZ=Asia/Shanghai
